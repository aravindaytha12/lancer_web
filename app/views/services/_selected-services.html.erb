<div class="container-fluid px-5">
    <div class="row">
      <div class="col-sm-12 col-md-12 col-lg-3">
        <p class="h4 pb-2 underline">Price Range</p>
        <div class="pt-2">
          <%= render 'services/search-sidebar' %>
        </div>
      </div>
      <div class="col"></div>
      <div class="col-sm-12 col-md-12 col-lg-8">
        <div class="row">
          <%= render partial: 'shared/services-row', locals: {services: @services} %>
        </div>
      </div>
    </div>
</div>

<div class="pagination text-success justify-content-center mt-5">
  <%= paginate @services %>
</div>